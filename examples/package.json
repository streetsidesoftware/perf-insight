{
  "name": "test-perf-insight",
  "version": "1.0.0",
  "description": "Example perf tests.",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -p . --watch",
    "test": "pnpm test:js",
    "test:all": "pnpm test:js && pnpm test:register",
    "test:js": "perf-insight example --suite map -t 500",
    "test:register": "perf-insight example --suite map -t 500 --register jiti/register --file **/*.perf.mts",
    "prepare": "pnpm build"
  },
  "author": {
    "name": "Street Side Software",
    "url": "https://github.com/streetsidesoftware"
  },
  "bugs": {
    "url": "https://github.com/streetsidesoftware/perf-insight/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/perf-insight#readme",
  "license": "MIT",
  "dependencies": {
    "cspell-trie-lib": "^9.2.2",
    "lorem-ipsum": "^2.0.8",
    "perf-insight": "workspace:*"
  }
}
